{
    "main": "./app/main.js",
    "devDependencies": {
        "electron": "^25.0.0",
        "electron-builder": "^24.0.0"
    },
    "license": "GNU GPLv3",
    "build": {
        "appId": "Ahmyth.android.rat",
        "productName": "AhMyth",
        "directories": {
            "output": "dist"
        },
        "files": [
            "**/*",
            "!dist/**/*",
            "!build/**/*",
            "!*.md",
            "!package-lock.json"
        ],
        "win": {
            "publisherName": "AhMyth",
            "icon": "build/icon.ico",
            "target": [
                {
                    "target": "nsis",
                    "arch": ["x64", "ia32"]
                }
            ]
        },
        "mac": {
            "icon": "build/icon.icns",
            "target": [
                {
                    "target": "dmg",
                    "arch": ["x64", "arm64"]
                }
            ]
        },
        "linux": {
            "icon": "build/icon.png",
            "target": [
                {
                    "target": "AppImage",
                    "arch": ["x64"]
                },
                {
                    "target": "deb",
                    "arch": ["x64", "ia32"]
                },
                {
                    "target": "rpm",
                    "arch": ["x64"]
                }
            ],
            "category": "Development"
        },
        "asarUnpack": "**/app/Factory/**/*"
    },
    "scripts": {
        "start": "npx electron --no-sandbox ./app",
        "start-root": "npx electron --no-sandbox ./app",
        "start-user": "npx electron ./app",
        "clean": "rm -rf ./dist",
        "build": "npm run build:all",
        "build:all": "npm run build:linux && npm run build:win && npm run build:mac",
        "build:linux": "npm run build:linux64",
        "build:linux32": "npx electron-builder --linux --ia32",
        "build:linux64": "npx electron-builder --linux --x64",
        "build:linux:appimage": "npx electron-builder --linux AppImage",
        "build:win": "npm run build:win64",
        "build:win32": "npx electron-builder --win --ia32",
        "build:win64": "npx electron-builder --win --x64",
        "build:mac": "npm run build:mac-intel && npm run build:mac-arm",
        "build:mac-intel": "npx electron-builder --mac --x64",
        "build:mac-arm": "npx electron-builder --mac --arm64",
        "dev": "npm run start",
        "dist": "npm run build:all",
        "postinstall": "npx electron-builder install-app-deps"
    },
    "dependencies": {},
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AbdalrohmanGitHub/AhMyth-AhMyth-Android-RAT.git"
    },
    "keywords": [
        "android",
        "rat",
        "remote-access",
        "pentesting",
        "security",
        "electron",
        "cross-platform"
    ],
    "author": "AhMyth Team",
    "description": "AhMyth Android RAT - Cross-platform remote access tool for Android devices"
}
